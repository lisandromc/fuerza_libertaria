<!DOCTYPE html>
<html>
  <head>
    <title>FuerzaLibertaria</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag('application', media: 'all') %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />

    <%= javascript_include_tag('application') %>
    <script src="//maps.google.com/maps/api/js?key=AIzaSyBJ4nB1pabS1zdNUVpzvga00bZ4delLyAw"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
    <%= Gon::Base.render_data(camel_case: true, namespace: 'serverExports') %>
  </head>

  <body id="<%= "#{controller_name}_#{action_name}" %>" data-controller="<%= controller_name %>" data-action="<%= action_name %>">
    <%= yield %>
  </body>
</html>
